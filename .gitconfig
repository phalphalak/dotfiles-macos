[core]
	excludesfile = .gitignore_global
	editor = vim
[user]
	name = Roman Flammer
	email = phalphalak@gmail.com
[push]
	default = tracking
[color]
	ui = true
	status = auto
	diff = auto
	branch = auto
[color "branch"]
  	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	whitespace = white reverse
[color "status"]
	added = green
	changed = red
	untracked = yellow
